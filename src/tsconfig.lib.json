{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "outDir": "../tmp/lib-es2015/",
        "target": "es2015",
        "rootDir": "./",
        "baseUrl": "",
        "types": ["node", "googlemaps"]
    },
    "files": ["./index.ts"],
    "angularCompilerOptions": {
        "strictMetadataEmit": true,
        "skipTemplateCodegen": true,
        "genDir": "../tmp/lib-gen-dir/",
        "flatModuleOutFile": "google-maps-autocomplete.js",
        "flatModuleId": "@angular-material-extensions/google-maps-autocomplete",
        "annotateForClosureCompiler": true
    }
}
