{
    "extends": "./tsconfig.lib.json",
    "compilerOptions": {
        "target": "es5",
        "outDir": "../tmp/lib-es5/",
        "baseUrl": "",
        "types": ["googlemaps"]
    },
    "files": ["./index.ts"],
