  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "globals": {
      "google": {}
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/forms",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@angular/cdk",
      "@angular/material  ",
      "@types/jasmine",
      "@types/jest",
      "@types/node",
      "conventional-github-releaser",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
